    <children>
        <Label alignment="CENTER" layoutX="420.0" layoutY="150.0" text="工位" textAlignment="CENTER">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <TextField fx:id="field_username" layoutX="369.0" layoutY="237.0" />
        <TextField fx:id="field_password" layoutX="369.0" layoutY="338.0" />
        <Button fx:id="login_btn" layoutX="410.0" layoutY="433.0" mnemonicParsing="false" onAction="#login_btn_click" text="登录"> <!--管理员登录按钮，一切开始的地方-->
