<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="700.0" prefWidth="1050.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginAdminController">
    <children>
        <Label alignment="CENTER" layoutX="425.0" layoutY="93.0" prefHeight="50.0" prefWidth="185.0" text="欢迎登陆" textAlignment="CENTER">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <TextField fx:id="field_username" onMouseClicked="#onTextFieldClicked" layoutX="425.0" layoutY="199.0" style="-fx-pref-height: 50; -fx-pref-width: 200;" />
        <PasswordField fx:id="field_password" onMouseClicked="#onTextFieldClicked" layoutX="425.0" layoutY="300.0" style="-fx-pref-height: 50; -fx-pref-width: 200;" />
        <Button fx:id="login_btn" layoutX="443.0" layoutY="396.0" mnemonicParsing="false" onAction="#login_btn_click" style="-fx-pref-height: 70; -fx-pref-width: 150;" text="登录"> <!--管理员登录按钮，一切开始的地方-->
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Label layoutX="428.0" layoutY="171.0" text="用户名">
         <font>
            <Font size="20.0" />
         </font></Label>
        <Label layoutX="425.0" layoutY="272.0" text="密码">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Button fx:id="figure_login_btn" layoutX="443.0" layoutY="502.0" mnemonicParsing="false" onAction="#figure_login_btn_click" style="-fx-pref-height: 70; -fx-pref-width: 150;" text="指纹登录">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="register_btn" layoutX="443.0" layoutY="603.0" mnemonicParsing="false" onAction="#register_btn_click" style="-fx-pref-height: 70; -fx-pref-width: 150;" text="注册">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
