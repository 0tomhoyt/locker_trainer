<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="anchorPane" prefHeight="700.0" prefWidth="450.0" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.WorkerTrainingHistory">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="450.0">
        <items>
          <Button mnemonicParsing="false" onAction="#back" text="返回" />
            <Label prefWidth="200.0" />
            <RadioButton fx:id="btn_all" mnemonicParsing="false" text="全部" />
            <RadioButton fx:id="btn_train" mnemonicParsing="false" text="训练" />
            <RadioButton fx:id="btn_match" mnemonicParsing="false" text="比赛" />
        </items>
      </ToolBar>
      <ScrollPane layoutY="40.0" prefHeight="660.0" prefWidth="450.0">
         <content>
            <VBox fx:id="vBox" prefHeight="660.0" prefWidth="450.0" />
         </content>
      </ScrollPane>
   </children>
</fx:root>
