<AnchorPane fx:id="anchorPane" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RegisterController">
    <children>
        <Label alignment="CENTER" layoutX="369.0" layoutY="91.0" prefHeight="50.0" prefWidth="185.0" text="注册页面" textAlignment="CENTER">
