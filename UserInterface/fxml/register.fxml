         <font>
            <Font size="20.0" />
         </font></Label>
      <Button fx:id="login_btn" layoutX="393.0" layoutY="592.0" mnemonicParsing="false" onAction="#login_btn_click" prefHeight="70.0" prefWidth="150.0" style="-fx-pref-height: 80;" text="登录">
         <font>
            <Font size="24.0" />
         </font>
      </Button>

    </children>
</AnchorPane>
