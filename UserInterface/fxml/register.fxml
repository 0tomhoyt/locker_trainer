                <Font size="40.0" />
            </font>
        </Label>
        <TextField fx:id="field_username" layoutX="369.0" layoutY="197.0" prefHeight="50.0" prefWidth="200.0" style="-fx-pref-height: 50;" />
        <TextField fx:id="field_password" layoutX="368.0" layoutY="298.0" prefHeight="50.0" prefWidth="200.0" style="-fx-pref-height: 50;" />
        <TextField fx:id="field_password_again" layoutX="369.0" layoutY="403.0" prefHeight="50.0" prefWidth="200.0" style="-fx-pref-height: 50;" />
        <Button fx:id="register_btn" layoutX="393.0" layoutY="486.0" mnemonicParsing="false" onAction="#register_btn_click" prefHeight="70.0" prefWidth="150.0" style="-fx-pref-height: 80;" text="注册">
