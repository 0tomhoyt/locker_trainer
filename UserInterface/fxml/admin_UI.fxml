<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="800.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminUIController">
   <children>
      <ToolBar layoutX="5.0" prefHeight="40.0" prefWidth="1500.0">
        <items>
          <Button fx:id="logout_btn" mnemonicParsing="false" onAction="#logout_btn_click" style="-fx-pref-width: 150px; -fx-pref-height: 30px;" text="登出" />
        </items>
      </ToolBar>
      <AnchorPane layoutX="5.0" layoutY="40.0" prefHeight="750.0" prefWidth="1500.0">
          <children>
              <TabPane>
                  <tabs>
                  <Tab closable="false" style="-fx-pref-width: 100px; -fx-pref-height: 50px;" text="开始训练">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0">
                           <children>
                              <AnchorPane fx:id="start_training_page" />
                           </children>
                        </AnchorPane>
                    </content>
                  </Tab>
                  <Tab closable="false" style="-fx-pref-width: 150px; -fx-pref-height: 50px;" text="工人信息">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0">
                          <children>
                              <VBox fx:id="vBox_worker_info" prefHeight="660.0" prefWidth="900.0" />
                          </children>
                        </AnchorPane>
                    </content>
                  </Tab>
                  <Tab closable="false" style="-fx-pref-width: 150px; -fx-pref-height: 50px;" text="开锁信息">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0">
                           <children>
                              <AnchorPane fx:id="check_lock_history_page" />
                           </children>
                        </AnchorPane>
                    </content>
                  </Tab>
                  <Tab closable="false" style="-fx-pref-width: 150px; -fx-pref-height: 50px;" text="添加指纹">
                     <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0">
                           <children>
                              <AnchorPane fx:id="add_figurePrint_page" />
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </AnchorPane>
                     </content>
                  </Tab>
                  </tabs>
              </TabPane>
          </children>
      </AnchorPane>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets left="5.0" />
   </padding>
</AnchorPane>
